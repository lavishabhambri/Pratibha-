<div class="container">
  <%doctorsList.forEach((item, i)=> {%>
    <div class="card" style="width: 30%; margin: 60px; box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);">
      <img src="" alt="Doc image" style="width:100%">
      <h2 style="margin: 4% 4%;">Dr. <%=item.username  %></h2>
      <h3 class="card-text" style="font-size: 20px; color:#000; margin: 4%;">Doctor Hos. ID : <%= item.docId %></h3>
      <h3 class="card-text" style="font-size: 20px; color:#000; margin: 4%;">Qualification :<% item.qualification %> </h3>
      <h3 class="card-text" style="font-size: 20px; color:#000; margin: 4%;">Hospital : <% item.hospital %></h3>
      <h3 class="card-text" style="font-size: 20px; color:#000; margin: 4%;">Location : <%= item.location %></h3>
      <button class="btn btn-lg btn-dark" style="margin: 20px;" type="button" name="button">Contact</button>
  </div>
  <%});%>
</div>






