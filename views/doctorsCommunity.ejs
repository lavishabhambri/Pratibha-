<body>

  <header class="d-flex flex-column flex-md-row align-items-center p-3 px-md-4 mb-3 bg-dark border-bottom shadow-sm">
    <p class="h5 my-0 me-md-auto fw-normal"><span style=" font-size:30px; font-weight:bold;"><span style="color:#F68283;">PCOS</span>isters</span></p>
    <nav class="my-2 my-md-0 me-md-3">
      <a class="p-2 text-dark" href="/home"><span class="navbar-text">Home</span></a>
      <a class="p-2 text-dark" href="/home/blogpage"><span class="navbar-text">Community</span></a>
      <a class="p-2 text-dark" href="/yogadiet"><span class="navbar-text">Yoga</span></a>
      <a class="p-2 text-dark" href="https://pcosmlmodel-api.herokuapp.com/"><span class="navbar-text">Self Test</span></a>
      <a class="p-2 text-dark" href="/patient/dashboard/selector"><span class="navbar-text">Dashboard</span></a>
    </nav>


    <button class="nav_button" style="color: #000;" type="button" name="button"><i class="fab fa-google"></i><a href="/auth/google" style="color:#fff;"> Sign Up as Sister</a></button>
    <button class="nav_button" style="color: #000;" type="button" name="button"><i class="fab fa-google"></i><a href="/auth/googledoc" style="color:#fff;"> Sign Up as Doctor</a></button>
  </header>


  <div class="container" style="overflow:scroll; padding:5%;">
        <%doctorsList.forEach((item, i)=> {%>
          <%if(item.isDoctor===true){%>
        <form action="/patient/askAppointment" method="POST">
            <div class="card" style="width: 18rem; display:inline; margin:2% 3% ">
              <img class="card-img-top" style="height:100px;width:100px; margin-left:0px;" src="<%=item.pictureURL%>" alt="Doc image">
              <%if(item.certified===true){%><img src="/HOMEimages/certified.png" style="height:60px;width:60px; position:relative; right:60px; top:22px;" alt=""><%}%>
              <div class="card-body">
                <h6 class="card-title">Doctor: <%=item.username  %></h6>
                <p class="card-text" style="color:#000;">Doctor ID :<%= item.doctorID %></p>
                <p class="card-text" style="color:#000;">Qualification:<%= item.qualification %> </p>
                <p class="card-text" style="color:#000;">Hospital: <%= item.hospitalName %></p>
                <p class="card-text" style="color:#000;">Location: <%= item.address %></p>
                <input type="hidden" name="doctorId" value="<%= item.user_id %>">
                <!-- <a href="" type="" class="btn btn-success">Appointment Sent</a> -->
                <button type="submit" class="btn btn-primary" name="button">Ask Appointment</button>
              </div>
            </div>
        </form>
      <%}%>
        <%});%>
  </div>










